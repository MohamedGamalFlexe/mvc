@model List<Employee>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Salary</th>
            <th>JobTitle</th>
            <th>ImageURL</th>
            <th>Address</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Salary</td>
                <td>@student.JobTitle</td>
                <td>@student.ImageURL</td>
                <td>@student.Address</td>
                <td>
                    <a href="/Student/Details?id=@student.Id">Edit</a>
                </td>
            </tr>
        }
    </tbody>
</table>

