@model List<Student>

<h1>Get All Student</h1>


<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>ImageURL</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.ImageURL</td>
                <td>
                    <a href="/Student/Details?id=@student.Id">Edit</a>
                </td>
            </tr>
        }
    </tbody>
</table>